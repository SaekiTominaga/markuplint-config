{
	"name": "@w0s/markuplint-config",
	"version": "2.0.0-rc.1",
	"description": "markuplint configuration file used on w0s.jp",
	"keywords": [
		"markuplint",
		"markuplint-config"
	],
	"homepage": "https://github.com/SaekiTominaga/markuplint-config#readme",
	"bugs": {
		"url": "https://github.com/SaekiTominaga/markuplint-config/issues"
	},
	"license": "MIT",
	"author": "Saeki Tominaga",
	"files": [
		""
	],
	"main": ".markuplintrc",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SaekiTominaga/markuplint-config.git"
	},
	"scripts": {
		"test-valid": "markuplint -c __tests__/.markuplintrc __tests__/valid/**/*.html",
		"test-invalid": "markuplint -c __tests__/.markuplintrc __tests__/invalid/**/*.html",
		"test-parser": "markuplint -c __tests__/.markuplintrc __tests__/parser/**/*"
	},
	"devDependencies": {
		"@markuplint/ejs-parser": "^3.0.0-rc.1",
		"markuplint": "^3.0.0-rc.1"
	},
	"publishConfig": {
		"access": "public"
	}
}
